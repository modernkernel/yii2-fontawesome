{
    "name": "powerkernel/yii2-fontawesome",
    "description": "Font Awesome extension for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Harry Tang",
            "email": "harry@powerkernel.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/font-awesome":"~5.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "powerkernel\\fontawesome\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
